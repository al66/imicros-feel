{
    "name": "imicros-feel",
    "version": "0.0.2",
    "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
    "license": "MIT",
    "description": "DMN FEEL service for imicros-backend",
    "main": "index.js",
    "scripts": {
        "test": "jest --coverage --no-cache"
    },
    "jest": {
        "collectCoverageFrom": [
            "lib/*.js",
            "lib/util/*.js",
            "!node_modules/",
            "!/dev/",
            "!test/helper/*.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/dev/"
        ],
        "rootDir": ".",
        "roots": [
            "./test"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/al66/imicros-feel.git"
    },
    "keywords": [
        "imicros",
        "backend"
    ],
    "devDependencies": {
        "jest": "^28.1.0",
        "moleculer": "^0.14.21"
    },
    "dependencies": {
        "imicros-feel-interpreter": "^0.0.6",
        "uuid": "^8.3.2"
    }
}
